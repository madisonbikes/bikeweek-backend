{
  "name": "bikeweek-backend",
  "version": "1.1.2",
  "description": "Imports events from Gravity Forms form data into Sched",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "compile": "tsc --noEmit",
    "dist": "tsc && npm run genversion",
    "dev": "ts-node-dev --inspect -- src/index.ts",
    "fmt": "prettier --write .",
    "pm2-start": "npx --yes pm2 startOrRestart ecosystem.config.js --update-env",
    "pm2-stop": "npx --yes pm2 stop ecosystem.config.js",
    "lint": "eslint . --max-warnings=0",
    "check-package-updates": "npm-check-updates",
    "test": "NODE_ENV=test npx jest --maxWorkers=50%",
    "test-watch": "NODE_ENV=test npx jest --maxWorkers=25% --watch",
    "coverage": "NODE_ENV=test npx jest --maxWorkers=50% --coverage=true",
    "prepare": "husky",
    "genversion": "genversion -f -u dist/src/backend-version.js",
    "knip": "knip",
    "check": "npm run compile && npm run lint && npm run knip && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madisonbikes/bikeweek-backend.git"
  },
  "author": "Ben Sandee",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=20.x"
  },
  "bugs": {
    "url": "https://github.com/madisonbikes/bikeweek-backend/issues"
  },
  "homepage": "https://github.com/madisonbikes/bikeweek-backend#readme",
  "dependencies": {
    "@hono/node-server": "^1.13.8",
    "@hono/zod-validator": "^0.4.3",
    "bcryptjs": "^3.0.2",
    "build-url": "^6.0.1",
    "connect-redis": "^8.0.1",
    "connection-string": "^4.4.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "google-auth-library": "^9.15.1",
    "hono": "^4.7.4",
    "hono-sessions": "^0.7.1",
    "http-status-codes": "^2.3.0",
    "mongodb": "^6.14.2",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "redis": "^4.7.0",
    "set-interval-async": "^3.0.3",
    "superagent": "^10.1.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.22.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.24",
    "@types/superagent": "^8.1.9",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/parser": "^8.26.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "genversion": "^3.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "knip": "^5.45.0",
    "mongodb-memory-server": "^10.1.4",
    "npm-check-updates": "^17.1.15",
    "prettier": "3.5.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  }
}
