{
  "name": "bikeweek-backend",
  "version": "0.3.0",
  "description": "Imports events from CSV form data into Sched",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev --inspect -- src/index.ts",
    "pm2-start": "npx --yes pm2 startOrRestart ecosystem.config.js",
    "pm2-stop": "npx --yes pm2 stop ecosystem.config.js",
    "lint": "eslint '**/*.ts'",
    "check-package-updates": "npx npm-check-updates --reject '@types/node'",
    "test": "NODE_ENV=test npx jest --maxWorkers=50%",
    "test-watch": "NODE_ENV=test npx jest --maxWorkers=25% --watch",
    "coverage": "NODE_ENV=test npx jest --maxWorkers=50% --coverage=true",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madisonbikes/bikeweek-backend.git"
  },
  "author": "Ben Sandee",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18.x"
  },
  "bugs": {
    "url": "https://github.com/madisonbikes/bikeweek-backend/issues"
  },
  "homepage": "https://github.com/madisonbikes/bikeweek-backend#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "build-url": "^6.0.1",
    "connect-redis": "^7.0.1",
    "connection-string": "^4.3.6",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "google-auth-library": "^8.7.0",
    "helmet": "^6.0.1",
    "http-status-codes": "^2.2.0",
    "mongodb": "^5.1.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0",
    "redis": "^4.6.5",
    "reflect-metadata": "^0.1.13",
    "set-interval-async": "^3.0.3",
    "superagent": "^8.0.9",
    "ts-node": "^10.9.1",
    "tsyringe": "^4.7.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^10.0.2",
    "@tsconfig/node18": "^1.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.13.0",
    "@types/passport": "^1.0.12",
    "@types/passport-local": "^1.0.35",
    "@types/set-interval-async": "^1.0.0",
    "@types/superagent": "^4.1.16",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-autofix": "^1.2.3",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "mongodb-memory-server": "^8.12.2",
    "prettier": "2.8.7",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.3"
  }
