{
  "name": "bikeweek-backend",
  "version": "0.3.0",
  "description": "Imports events from CSV form data into Sched",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev --inspect -- src/index.ts",
    "pm2-start": "npx --yes pm2 startOrRestart ecosystem.config.js",
    "pm2-stop": "npx --yes pm2 stop ecosystem.config.js",
    "lint": "eslint '**/*.ts'",
    "check-package-updates": "npx npm-check-updates --reject '@types/node'",
    "test": "NODE_ENV=test npx jest --maxWorkers=50%",
    "test-watch": "NODE_ENV=test npx jest --maxWorkers=25% --watch",
    "coverage": "NODE_ENV=test npx jest --maxWorkers=50% --coverage=true",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madisonbikes/bikeweek-backend.git"
  },
  "author": "Ben Sandee",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18.x"
  },
  "bugs": {
    "url": "https://github.com/madisonbikes/bikeweek-backend/issues"
  },
  "homepage": "https://github.com/madisonbikes/bikeweek-backend#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "build-url": "^6.0.1",
    "connect-redis": "^7.1.1",
    "connection-string": "^4.4.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "google-auth-library": "^9.6.1",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "mongodb": "^6.3.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pino": "^8.18.0",
    "pino-pretty": "^10.3.1",
    "redis": "^4.6.12",
    "reflect-metadata": "^0.2.1",
    "set-interval-async": "^3.0.3",
    "superagent": "^8.1.2",
    "ts-node": "^10.9.2",
    "tsyringe": "^4.8.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^10.0.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.19.14",
    "@types/passport": "^1.0.12",
    "@types/passport-local": "^1.0.38",
    "@types/set-interval-async": "^1.0.0",
    "@types/superagent": "^8.1.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-autofix": "^1.2.3",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.6",
    "prettier": "3.2.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
